<div class='big_box'>
<% (1..9).each do |x| %>
<div id = 'square'>
  <% case %>
  <% when @users_moves.include?(x) %>
      X
  <% when @comps_moves.include?(x) %>
      O
  <% else %>
    <%= link_to x, game_path(@game, player_move: x) %>
  <% end %>
</div>
<% end %>
</div>

<h3><%= @message %></h3>

