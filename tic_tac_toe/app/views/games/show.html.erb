<h1>Playing Game</h1>

<%= render 'table' %>


  <b>Past Moves
  <% @moves.each do |move| %>
    <%= move.square_id %>
      <% if move.player_id %>
          X;
      <% else %>
            O;
      <% end %>
      <% end %>
      </b>
  <br>


<p><%= link_to 'New Game', new_game_path %></p>

<p><%= link_to 'Delete Game', @game, method: :delete  %></p>
